{
  "name": "webproxyapi",
  "version": "1.0.0",
  "description": "An OAuth secured API that manages translation from SameSite cookies to tokens",
  "scripts": {
    "lint": "npx eslint . --ext .ts,.tsx",
    "build": "rimraf dist && tsc --sourceMap true",
    "start": "node dist/express/app.js",
    "testLambdas": "./test/testLambdas.sh",
    "testHttpRemote": "./test/testHttpRemote.sh",
    "authorizationCodeGrant": "sls invoke local -f authorizationCodeGrant -p test/data/authorizationCodeGrantRequest.json",
    "refreshTokenGrant": "sls invoke local -f refreshTokenGrant -p test/data/refreshTokenGrantRequest.json",
    "expireRefreshToken": "sls invoke local -f expireRefreshToken -p test/data/expireRefreshTokenRequest.json",
    "expireSession": "sls invoke local -f expireSession -p test/data/expireSessionRequest.json",
    "package": "npm run buildRelease && rimraf .serverless && sls package --stage deployed",
    "buildRelease": "rimraf dist && tsc",
    "deploy": "sls deploy --package .serverless --stage deployed",
    "remove": "sls remove --stage deployed"
  },
  "author": "Gary Archer",
  "license": "MIT",
  "dependencies": {
    "@middy/core": "1.4.0",
    "@middy/http-cors": "^1.4.0",
    "axios": "^0.21.1",
    "cookie": "^0.4.1",
    "cookie-encrypter": "^1.0.1",
    "fs-extra": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/cookie": "^0.4.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.21.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "serverless": "^2.33.0",
    "serverless-domain-manager": "4.1.0",
    "serverless-plugin-scripts": "^1.0.2",
    "tunnel-agent": "^0.6.0",
    "typescript": "^4.2.2"
  }
}
