{
  "name": "tokenhandlerapi",
  "version": "1.0.0",
  "description": "An API that performs OAuth work for the final SPA and issues secure SameSite cookies",
  "scripts": {
    "lint": "npx eslint . --ext .ts,.tsx",
    "start": "nodemon -e ts --exec \"IS_LOCAL=true ts-node --files src/express/app.ts\"",
    "build": "rimraf dist && tsc --outDir dist",
    "buildRelease": "rimraf dist && tsc --outDir dist --sourceMap false",
    "startRelease": "node dist/express/app.js",
    "test": "./test/runHttpWorkflow.sh"
  },
  "author": "Gary Archer",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "base64url": "^3.0.1",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "guid-typescript": "^1.0.9",
    "jsonwebtoken": "^8.5.1",
    "proxy-agent": "^5.0.0",
    "url-parse": "^1.5.1"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^14.14.10",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "4.5.5"
  }
}
