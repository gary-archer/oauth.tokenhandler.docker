{
  "name": "webproxyapi",
  "version": "1.0.0",
  "description": "An OAuth secured API that manages translation from SameSite cookies to tokens",
  "scripts": {
    "lint": "npx eslint . --ext .ts,.tsx",
    "start": "nodemon -e ts --exec \"npm run build\"",
    "build": "ts-node --files src/express/app.ts",
    "buildRelease": "rimraf dist && tsc --outDir dist --sourceMap false",
    "startRelease": "node dist/express/app.js",
    "testHttp": "./test/runHttpWorkflow.sh",
    "testLambda": "./test/runLambdaWorkflow.sh",
    "package": "npm run buildRelease && rimraf .serverless && sls package --stage deployed",
    "customPackager": "ts-node deployment/customPackager.ts",
    "deploy": "sls deploy --stage deployed --package .serverless",
    "remove": "sls remove --stage deployed"
  },
  "author": "Gary Archer",
  "license": "MIT",
  "dependencies": {
    "@middy/core": "1.4.0",
    "@middy/http-cors": "^1.4.0",
    "axios": "^0.21.1",
    "cookie": "^0.4.1",
    "cookie-encrypter": "^1.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "tunnel-agent": "^0.6.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/aws-lambda": "^8.10.64",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "adm-zip": "^0.5.5",
    "eslint": "^7.21.0",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "serverless": "^2.40.0",
    "serverless-domain-manager": "4.1.0",
    "serverless-plugin-scripts": "^1.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.2"
  }
}
